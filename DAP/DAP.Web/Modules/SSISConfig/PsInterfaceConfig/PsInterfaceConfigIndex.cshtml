@{
    ViewData["Title"] = Serenity.LocalText.Get("Db.SSISConfig.PsInterfaceConfig.EntityPlural");
}

@section ContentHeader {
    <h1>@ViewData["Title"]<small></small></h1>
}

<div class="alert alert-info alert-dismissable">
    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
    <p>
        Invoices will not be posted to PeopleSoft during the periods ClosingDateFrom/ClosingDateTo. i.e. If ClosingDateTo is 6/1/2021 Invoices will post starting 6/2/2021.
    </p>

</div>

<div id="GridDiv"></div>

<script type="text/javascript">
    jQuery(function () {
        new DAP.SSISConfig.PsInterfaceConfigGrid($('#GridDiv'), {}).init();

        Q.initFullHeightGridPage($('#GridDiv'));
    });
</script>